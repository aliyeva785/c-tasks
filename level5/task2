#include <iostream>
using namespace std;
void f(int &a, int &b) {
    int temp = a;
    a = b;
    b = temp;
}
int main() {
    int a, b;
    cout << "Birinci ededi daxil edin: ";
    cin >> a;
    cout << "Ikinci ededi daxil edin: ";
    cin >> b;
    f(a, b);
    cout << "Yerleri deyisdikden sonra:" << endl;
    cout << "a = " << a << endl;
    cout << "b = " << b << endl;
    return 0;
}
