#include <iostream>
using namespace std;

void f(int a, int b) {
    if (a > b)
        cout << "Boyuk eded = " << a << endl;
    else if (a < b)
        cout << "Boyuk eded = " << b << endl;
    else
        cout << "Ededler beraberdir" << endl;
}
int main() {
    int a, b;
    cout << "Ededleri daxil et: ";
    cin >> a >> b;
    f(a, b);
    return 0;
}
