#include <iostream>
using namespace std;

bool pal(int n) {
    int t = n, rev = 0;
    while (t > 0) {
        rev = rev * 10 + t % 10;
        t /= 10;
    }
    return rev == n;
}

void pal(int limit) {
    for (int i = 1; i <= limit; i++) {
        if (palindromMu(i)) cout << i << " ";
    }
    cout << endl;
}

int main() {
    int limit = 1000;
    pal(limit);
    return 0;
}
